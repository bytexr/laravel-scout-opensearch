{
    "name": "bytexr/laravel-scout-opensearch",
    "description": "Laravel Scout OpenSearch Engine",
    "keywords": ["opensearch", "laravel", "search", "scout", "aws"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ByteXR\\LaravelScoutOpenSearch\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Eddie Rusinskas",
            "email": "info@imphus.com"
        }
    ],
    "require": {
        "laravel/scout": "^10.9",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "opensearch-project/opensearch-php": "^2.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ByteXR\\LaravelScoutOpenSearch\\LaravelScoutOpenSearchServiceProvider"
            ]
        }
    },
    "require-dev": {
        "laravel/pint": "^1.7",
        "pestphp/pest": "^2.5"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    }
}
